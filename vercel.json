{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "tugeocercas.com" }
      ],
      "destination": "https://www.tugeocercas.com/$1",
      "permanent": true
    }
  ],

  "routes": [
    { "src": "^/icons/(.*)$", "dest": "/icons/$1" },
    { "src": "^/manifest\\.webmanifest$", "dest": "/manifest.webmanifest" },

    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
