{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.tugeocercas.com" }],
      "destination": "https://tugeocercas.com/$1",
      "permanent": true
    }
  ],
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "^(.*)$", "dest": "/index.html" }
  ]
}
